<!-- Loop over each object in the blogs array -->
{{#each blogs as |blog| }}
<div class="row mb-4 blog">
  <div class="col-md-5">
    <h2>
      {{{get_emoji}}}
      <a href="/blog/{{blog.id}}">{{blog.description}}</a>
    </h2>
  </div>
  <div class="col-md-7">
    <p>
      {{blog.content}}
    </p>
  </div>
</div>
{{/each}}
